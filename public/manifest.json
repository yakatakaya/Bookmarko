{
    "name": "Bookmarko",
    "description": "Bookmark manager",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "tabs",
      "bookmarks",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "ribbon_128.png"
    },
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ]
  }